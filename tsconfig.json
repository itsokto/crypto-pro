{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "declaration": true,
    "lib": ["ES2020", "DOM"],
    "types": ["node", "vitest", "cadesplugin"],
    "strict": false,
    "sourceMap": true,
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types", "./types"]
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
